    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "scripts": {
    "build": "yarn run build:api && yarn run build:ui",
    "build:api": "babel src/api -d dist/api",
    "build:tests": "babel test/ -d lib/",
    "build:ui": "webpack",
    "build:watch": "concurrently \"yarn run build:api --watch\" \"yarn run build:ui --watch\"",
    "flow": "cd src && flow; cd ..",
    "lint": "eslint src",
    "lint:fix": "yarn run prettier --write; yarn run lint --fix",
    "prettier": "prettier -l src/**/*.js",
    "postinstall": "cd src/ && flow-typed install --ignoreDeps dev; cd ..",
    "start": "yarn build && node dist/api/server/index.js",
    "test": "yarn run build:api && yarn run build:tests && jest lib/"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/setupTests.js"
    ]
  }
}
