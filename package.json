    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "empty": "^0.10.1",
    "enzyme": "^3.5.1",
    "enzyme-adapter-react-16": "^1.4.0",
